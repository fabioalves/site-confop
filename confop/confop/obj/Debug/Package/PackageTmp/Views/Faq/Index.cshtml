@model IEnumerable<confop.Models.Faq>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<link href="@Url.Content("~/Content/format.css")" rel="stylesheet" type="text/css" />
<script src="@Url.Content("~/Scripts/javascript.js")" type="text/javascript"></script>
<h1>Perguntas Frequentes</h1>
<br />
<div id="wrapper">
@foreach (var item in Model)
{
		<div class="accordionButton">@item.Pergunta</div>
		<div class="accordionContent">@item.Resposta<br /><br /><br /><br /><br /></div>		
}
</div>
    